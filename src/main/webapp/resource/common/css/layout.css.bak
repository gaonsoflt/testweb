@charset "utf-8";

.mt0{margin-top:0 !important}

.wrapper {width:100%; height:100%; position:relative; padding-bottom:50px;}
/* 전체 메뉴 관련 */
/* .wrapper .total_menu {position:fixed; top:42px; right:0; z-index:501;} */
.wrapper .total_menu {position:fixed; top:49px; right:0; z-index:501;}
.wrapper .contentBG {display:none;position:absolute; top:0; left:0; width:100%; height:100%; background-color:#000; opacity: 0.5; z-index:300; cursor:default;}
.wrapper .side_area {position:relative; position:fixed; top:0; right:-398px; z-index:502; width:400px; height:100%; background-color:#3f2f2a;}
.wrapper .side_GNB {padding:62px 0 0 88px; text-align:left;}
.wrapper .side_GNB .menu {padding:20px 0 49px; background:url('../images/common/bg_rowbar.png') no-repeat 0 0;}
.wrapper .side_GNB .menu > li {width:200px; height:40px; margin-top:8px;}
/*.wrapper .side_GNB .menu > li > a {display:block; overflow:hidden; height:40px; background:url('../images/common/gnb_off.png') no-repeat 0 2px; text-indent:-9999px;}*/
.wrapper .side_GNB .menu > li > a {display:block; overflow:hidden; height:40px; background:url('../images/common/gnb_off_v150331.png') no-repeat 0 2px; text-indent:-9999px;}
.wrapper .side_GNB .menu > li > a.menu2 {background-position:0 -46px;}
.wrapper .side_GNB .menu > li > a.menu3 {background-position:0 -95px;}
.wrapper .side_GNB .menu > li > a.menu4 {background-position:0 -143px;}
.wrapper .side_GNB .menu > li.on > a,
/*.wrapper .side_GNB .menu > li > a:hover {background-image:url('../images/common/gnb_on.png');}*/
.wrapper .side_GNB .menu > li > a:hover {background-image:url('../images/common/gnb_on_v150331.png');}/* 150331 업데이트 */
.wrapper .side_GNB .menu > li:first-child {margin:0;}
