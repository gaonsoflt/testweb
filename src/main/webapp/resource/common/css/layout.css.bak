.wrapper .side_GNB .menu > li ul.sub_menu {padding-top:25px; display:none;}
.wrapper .side_GNB .menu > li ul.sub_menu > li {height:26px;margin-top:10px; }
.wrapper .side_GNB .menu > li ul.sub_menu > li a {display:block; font-size:14px; line-height:16px; padding:5px 11px; color:#dcc6bf; letter-spacing:-0.07em;}
.wrapper .side_GNB .menu > li ul.sub_menu > li.on a,
.wrapper .side_GNB .menu > li ul.sub_menu > li a:hover {color:#3f2f2a; background:url('../images/common/bg_gnb.gif') no-repeat 50% 0;}
.wrapper .side_GNB .menu > li ul.sub_menu > li:first-child {margin:0;}
